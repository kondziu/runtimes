function three() <- 3;
function hello(i) <- print("Hello ~ ", i);
function world(j, k) <- print("world ~ ~!\n", j, k);

function main() <-
begin
  let i = three();
  hello(i);
  let j = 10;
  world(4, j);
end;

main()

